{
  "presets": ["next/babel"],
  "plugins": [
    ["styled-components", {"ssr": true}],
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "~/styles": "./src/styles",
          "~/components": "./src/components",
          "~/domain": "./src/domain",
        }
      }
    ]
  ]
}
